    "@ig/engine-be-models": "*",
    "@ig/engine-db": "*",
    "@ig/lib": "*",
    "express": "^5.2.1",
    "graphql": "^16.12.0"
  },
  "devDependencies": {
    "@ig/lint": "*",
    "@ig/vitest": "*",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.15.0",
